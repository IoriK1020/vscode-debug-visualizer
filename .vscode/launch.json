{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/extension",
				"${workspaceFolder}\\example-project"
			],
			"outFiles": ["${workspaceFolder}/extension/out/**/*.js"],
			"preLaunchTask": "npm: dev - extension"
		}
	]
}
